<c74object name="pnp.bpm~">

	<digest>
		Beats per minute estimator
	</digest>
	
	<description>
		<m>pnp.bpm~</m> estimates the BPM of the onsets of an incoming audio signal.
	</description>
	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Austin Franklin</metadata>
		<metadata name="tag"><PnP class="Maxtools"></PnP></metadata>
		<metadata name="tag">Audio</metadata>
		<metadata name="tag">Descriptor</metadata>
		<metadata name="tag">FFT</metadata>
	</metadatalist>
	
	<!--INLETS-->
	<inletlist>
		<inlet id="0" type="signal">
			<digest>Audio Signal Input</digest>
		</inlet>
	</inletlist>

	<!--OUTLETS-->
	<outletlist>
		<outlet id="0" type="int">
			<digest>Estimated BPM</digest>
		</outlet>
	</outletlist>
	
	<!--ARGUMENTS-->
	
	<!--MESSAGES-->
	<methodlist>
		<method name="signal">
			<arglist>
				<arg name="parameter-settings" optional="0" type="signal"/>
			</arglist>
			<digest>
				Function depends on inlet
			</digest>
			<description>
				In left inlet: Monoaural <m>signal</m>.
			</description>
		</method>
	</methodlist>
	
	<!--SEEALSO-->
	<seealsolist>
		<seealso name="average~"/>
		<seealso name="peak~"/>
		<seealso name="pnp.beat~"/>
	</seealsolist>
	
</c74object>